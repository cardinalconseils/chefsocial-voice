{
  "name": "chefsocial-voice",
  "version": "1.0.0",
  "description": "ChefSocial AI-powered restaurant marketing platform with voice capabilities",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "build": "npm run build:admin",
    "build:admin": "cd admin-panel && npm run build",
    "deploy": "vercel --prod",
    "lint": "eslint src/ --ext .js",
    "format": "prettier --write src/"
  },
  "keywords": [
    "restaurant",
    "ai",
    "voice",
    "marketing",
    "social-media",
    "automation"
  ],
  "author": "ChefSocial Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "sqlite3": "^5.1.7",
    "stripe": "^17.3.1",
    "twilio": "^5.4.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chefsocial/chefsocial-voice.git"
